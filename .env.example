# LTA DataMall API Key
VITE_LTA_DATAMALL_API_KEY="your-api-key-here"

# Mock API Configuration
# Set to "true" to use Mockoon instead of real API
VITE_USE_MOCK_API=false

# Mockoon server URL (default: http://localhost:3001)
VITE_MOCK_API_URL=http://localhost:3001

# Throttle interval for bus arrival data fetching (in milliseconds)
# Default: 45000 (45 seconds)
VITE_THROTTLE_INTERVAL_MS=45000

# Bus stops refresh interval (in days)
# Default: 7 days
VITE_BUS_STOPS_REFRESH_DAYS=7